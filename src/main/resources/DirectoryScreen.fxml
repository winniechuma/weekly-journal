<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="239.0" prefWidth="270.0">
      <children>
         <TextArea fx:id="DirectoryTextArea" layoutX="20.0" layoutY="76.0" prefHeight="43.0" prefWidth="229.0" />
         <Label layoutX="81.0" layoutY="29.0" text="Enter Directory:">
            <font>
               <Font name="Bernard MT Condensed" size="18.0" />
            </font>
         </Label>
         <Button fx:id="DirectoryEnter" layoutX="183.0" layoutY="182.0" mnemonicParsing="false" text="Enter">
            <font>
               <Font name="Bernard MT Condensed" size="14.0" />
            </font>
         </Button>
         <Button fx:id="DirectoryBack" layoutX="38.0" layoutY="182.0" mnemonicParsing="false" text="Back">
            <font>
               <Font name="Bernard MT Condensed" size="14.0" />
            </font>
         </Button>
      </children></AnchorPane>
</Scene>
